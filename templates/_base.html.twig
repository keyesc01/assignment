    <title>Martial Arts Club {% block title %}{% endblock %}</title>
    <style>
        @import "css/style.css";
        @import "css/nav.css";
    </style>
</head>
<body>
<section>
    {% if username %}
        <p>
           Logged in as : {{ username }}
        </p>
        <hr>
    {% endif %}

</section>


<nav>
    <ul>
        <li>
            <a href="index" class="{% block indexNavStyle %}{% endblock %}">Home page</a>
        </li>
        <li>
            <a href="members" class="{% block aboutNavStyle %}{% endblock %}">Member Login</a>
        </li>
        <li>
            <a href="days" class="{% block daysNavStyle %}{% endblock %}">Days</a>
        </li>
    </ul>
</nav>


{% block main %}
{% endblock %}

<footer>
    2016 Ciaran Keyes WebFrameWork Project
</footer>
</body>
</html>
